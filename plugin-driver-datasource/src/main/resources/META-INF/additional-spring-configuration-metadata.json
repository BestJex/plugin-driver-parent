{
  "properties": [
    {
      "name": "plugin.run-mode",
      "type": "java.lang.String",
      "defaultValue": "dev",
      "description": "plugin run mode dev/development/prod/deployment."
    },
    {
      "name": "plugin.store-type",
      "type": "java.lang.String",
      "defaultValue": "local",
      "description": "plugin file store type local/minio."
    },
    {
      "name": "plugin.minio.endpoint",
      "type": "java.lang.String",
      "description": "when plugin.store-type=minio, minio.endpoint."
    },
    {
      "name": "plugin.minio.access-key",
      "type": "java.lang.String",
      "description": "when plugin.store-type=minio, minio.accessKey."
    },
    {
      "name": "plugin.minio.secret-key",
      "type": "java.lang.String",
      "description": "when plugin.store-type=minio, minio.secretKey."
    },
    {
      "name": "plugin.plugin-path",
      "type": "java.lang.String",
      "defaultValue": "plugins",
      "description": "plugin path."
    },
    {
      "name": "plugin.plugin-config-file-path",
      "type": "java.lang.String",
      "defaultValue": "pluginConfigs",
      "description": "plugin config file path."
    },
    {
      "name": "plugin.plugin-init-load",
      "type": "java.lang.String",
      "description": "plugin init load, Fuzzy matching, if '*', load all, also can user pluginId@version Specified version, default use max version."
    }
  ]
}